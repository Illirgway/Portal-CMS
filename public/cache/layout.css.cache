body {font-family: 'Dosis', sans-serif;}.logo img {width: 100%;margin: 10px 0;}.header {border-bottom: 5px solid #C9C9C9;}.slideshow .width_100 {margin: 0;width: 100% !important;}.content-container {border-top: 5px solid #8CD3F6;background: #2071B6;color: white;}.questions {background: #C9C9C9;color: white;font-size: 1.8em;padding: 10px;}.button,.questions .button {text-decoration: none;background: #ffffff;border: 1px solid #2071B6;color: #2071B6;padding: 10px 0px;text-align: center;display: block;}.footer {padding: 0;}.footer .metanavigation {background: #8CD3F6;}.footer .end {background: #2071B6;}.footer .end .text {text-align: left;color: white;font-weight: 600;font-size: 1.2em;padding-top: 30px;}.footer .end .image {text-align: right;}.navigation {margin-top: 0.6em;}.navigation ul {margin: 0;padding: 0;list-style: none;}.navigation ul ul {display: none;}.navigation a {display: block;float: left;font-size: 1.2em;font-weight: 600;text-decoration: none;color: #000000;padding: 15px 10px;}.subnavigation-window {padding: 15px;background: #8CD3F6;box-shadow: 0 0 5px rgba(0,0,0,0.5);}.subnavigation-window .col2 {height: 400px;color: white;}.subnavigation-window .col2 h2 {font-weight: 600;color: white;}.subnavigation-window .col1 a {display: block;color: #2071B6;padding: 10px;text-decoration: none;font-size: 1.2em;}.subnavigation-window .col1 a.active,.subnavigation-window .col1 a:hover {color: #ffffff;background: #2071B6;}.row {margin-left: 0px;margin-right: 0px;}.metanavigation {padding: 10px;}.metanavigation ul {margin: 0;padding: 0;list-style: none;}.metanavigation ul a {text-decoration: none;color: #2071B6;font-weight: 600;display: block;margin: 0 15px;font-size: 1em;padding-bottom: 10px;}.metanavigation ul a:hover {color: #ffffff;}.metanavigation ul ul a {text-decoration: none;color: #2071B6;font-weight: 300;padding: 5px 0;font-size: 0.9em;}.metanavigation ul ul a:hover {color: #ffffff;}.metanavigation ul li {float: left;}.metanavigation ul li ul li {display: block;clear: both;}.metanavigation ul h2,.metanavigation ul p,.metanavigation ul img {display: none;}.sub_navigation_left {width: 33.33333333%;position: relative;min-height: 1px;float: left;}.sub_navigation_content {width: 100%;position: relative;min-height: 1px;float: left;}.content.sub_navigation_content {width: 66.66666667%;position: relative;min-height: 1px;float: left;}.content {padding: 10px;font-size: 1.2em;}.sub_navigation_left ul {margin: 0;padding: 0;list-style: none;}.sub_navigation_left a {display: block;padding: 10px;color: #ffffff;text-decoration: none;}.sub_navigation_left a.active_1,.sub_navigation_left a:hover {background: #8CD3F6;color: #2071B6;}.questions .col1,.questions .col2 {padding-top: 0.3em;}.contactform label {display: block;font-weight: 300;}.contactform input,.contactform textarea {width: 100%;padding: 1%;border: 1px solid #ffffff;color: #2071B6;}.contactform textarea {height: 150px;}.contactform .error {background: #FF5757;color: #ffffff;}.contactform #form_postal_code_text {width: 10%;margin-right: 1%;}.contactform #form_city_text {width: 89%;}.contactform .success {background: #80E800;padding: 2%;text-align: center;margin-top: 2%;color: black;font-weight: 700;}.subnavigation-window .col2 .black-overlay {background: rgba(0,0,0,0.5);padding: 4%;}.subnavigation-window .col2 p {font-size: 1.5em;}